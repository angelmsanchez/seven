{
  "name": "seven",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:docker": "docker build -t seven:prod .",
    "build:docker:dev": "docker build -t seven:dev --build-arg configuration='' .",
    "build:prod": "ng build --prod",
    "build:webpack": "cross-env NODE_ENV=development webpack",
    "build:webpack:prod": "cross-env NODE_ENV=production webpack",
    "docker:run": "docker run -p 80:80 seven:prod",
    "docker:run:dev": "docker run -p 80:80 seven:dev",
    "docker:stop": "docker container stop NAME",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --poll=500",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --progress --watch --hot --open --port 8080 --colors",
    "start:prod": "yarn run build:webpack:prod && http-server dist -p 4200 -o -g",
    "start:serve": "ng serve --open",
    "start:server": "json-server --watch db.json",
    "start:webpack-dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js --progress --watch --hot --open --port 8080 --colors",
    "test": "ng test",
    "webpack-dev": "cross-env NODE_ENV=development webpack --config config/webpack.dev.js",
    "webpack-prod": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.0",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/forms": "^7.1.0",
    "@angular/http": "^7.1.0",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "^7.1.0",
    "@angular/platform-browser-dynamic": "^7.1.0",
    "@angular/pwa": "^0.11.4",
    "@angular/router": "^7.1.0",
    "@angular/service-worker": "^7.1.0",
    "core-js": "2.5.4",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.2",
    "@angular/cli": "7.0.2",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/language-service": "^7.1.0",
    "@babel/core": "^7.2.0",
    "@ngtools/webpack": "7.0.2",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "angular-router-loader": "^0.8.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "codelyzer": "4.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "json-server": "^0.14.0",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-example-lib": "^0.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "protractor": "5.4.0",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "3.1.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-merge": "^4.1.5"
  }
}
